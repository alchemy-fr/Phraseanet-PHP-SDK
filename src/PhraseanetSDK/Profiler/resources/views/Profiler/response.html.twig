<div id="response_{{ index }}" class="tab-pane">
    <h5>Informations</h5>
    <table>
        <tbody>
            <tr>
                <th>Code</th>
                <td>{{ response.statusCode }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ response.reasonPhrase }}</td>
            </tr>
        </tbody>
    </table>

    <h5>Headers</h5>
    <table>
        <tbody>
        {% for header,value in response.headers %}
            <tr>
                <th>{{ header }}</th>
                <td>{{ value|join('<strong>, </strong>') }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <h5>Content</h5>
    <pre class="prettyprint linenums"><code class="language-javascript  ">{{ responseContent|nl2br }}</code></pre>
</div>
